package classe14_08;

public class coelho {
	
	    private String nome;
	    private String cor;
	    private int idade;
	    private double peso;

	    public coelho() {
	    }

	    public String getNome() {
	        return nome;
	    }

	    public void setNome(String nome) {
	        this.nome = nome;
	    }

	    public String getCor() {
	        return cor;
	    }

	    public void setCor(String cor) {
	        this.cor = cor;
	    }

	    public int getIdade() {
	        return idade;
	    }

	    public void setIdade(int idade) {
	        this.idade = idade;
	    }

	    public double getPeso() {
	        return peso;
	    }

	    public void setPeso(double peso) {
	        this.peso = peso;
	    }

	    public static String correr() {
	        return "Correndo";
	    }

	    public String pular() {
	        return "Pulando";
	    }

	    public String comer() {
	        return "Comendo";
	    }
	    
	}


